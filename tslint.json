{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": {
            "options":"tabs"
        },
        "object-literal-sort-keys": false,
        "space-before-function-paren": false,
        "interface-name": false,
        "trailing-comma": false,
        "no-console": false,
        "one-line": false,
        "max-line-length": false,
        "no-trailing-whitespace": [ true, "ignore-blank-lines" ]
    },
    "rulesDirectory": []
}